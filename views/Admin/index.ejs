  </div>
  <div class="CoreScripts">
    <!--   Core JS Files   -->
    <script src="/Admin/Files/javascripts/Assets/core/jquery.3.2.1.min.js"></script>
    <script src="/Admin/Files/javascripts/Assets/core/popper.min.js"></script>
    <script src="/Admin/Files/javascripts/Assets/core/bootstrap.min.js"></script>
    <!-- jQuery UI -->
    <script src="/Admin/Files/javascripts/Assets/plugin/jquery-ui-1.12.1.custom/jquery-ui.min.js"></script>
    <script src="/Admin/Files/javascripts/Assets/plugin/jquery-ui-touch-punch/jquery.ui.touch-punch.min.js"></script>

    <!-- jQuery Scrollbar -->
    <script src="/Admin/Files/javascripts/Assets/plugin/jquery-scrollbar/jquery.scrollbar.min.js"></script>
    <script src="/Admin/Files/javascripts/Assets/atlantis.min.js"></script>
    <script src="/Admin/Files/javascripts/Admin/Admin.js"></script>
    <script>
      clickHash("<%=url%>");
    </script>
