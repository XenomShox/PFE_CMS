            <li class="nav-item">
              <a data-toggle="collapse" href="#base">
                <i class="fas fa-layer-group"></i>
                <p>API Manager</p>
                <span class="caret"></span>
              </a>
              <div class="collapse" id="base">
                <ul class="nav nav-collapse">
                  <li>
                    <a href="#"  loading-Link="/Admin/ApiSchemas">
                      <span class="sub-item">API Schemas</span>
                    </a>
                  </li>
                  <li>
                    <a href="#"  loading-Link="/Admin/Database">
                      <span class="sub-item">DataBase</span>
                    </a>
                  </li>
                </ul>
              </div>
            </li>
            <li class="nav-item">
              <a data-toggle="collapse" href="#UsersManager" >
                <i class="fas fa-user-cog"></i>
                <p>Users Managers</p>
                <span class="caret"></span>
              </a>
              <div class="collapse" id="UsersManager">
                <ul class="nav nav-collapse">
                  <li>
                    <a href="#"  loading-Link="/Admin/UsersList">
                      <span class="sub-item">Users List</span>
                    </a>
                  </li>
                  <li>
                    <a href="#"  loading-Link="/Admin/Roles">
                      <span class="sub-item">Roles</span>
                    </a>
                  </li>
                </ul>
              </div>
            </li>
            <li class="nav-item">
              <a href="#" loading-Link="/Admin/FilesManager">
                <i class="fas far fa-list"></i>
                <p>Files Manager</p>
              </a>
            </li>
            <li class="nav-item">
              <a data-toggle="collapse" href="#WebSiteSettings" >
                <i class="fas fa-cog"></i>
                <p>WebSite Settings</p>
                <span class="caret"></span>
              </a>
              <div class="collapse" id="WebSiteSettings">
                <ul class="nav nav-collapse">
                  <li>
                    <a href="#"  loading-Link="/Admin/General">
                      <span class="sub-item">General</span>
                    </a>
                  </li>
                  <li>
                    <a href="#"  loading-Link="/Admin/DataBaseSettings">
                      <span class="sub-item">DataBase</span>
                    </a>
                  </li>
                  <li>
                    <a href="#"  loading-Link="/Admin/Writing">
                      <span class="sub-item">Writing</span>
                    </a>
                  </li>
                  <li>
                    <a href="#"  loading-Link="/Admin/Reading">
                      <span class="sub-item">Reading</span>
                    </a>
                  </li>
                  <li>
                    <a href="#"  loading-Link="/Admin/Templates">
                      <span class="sub-item">DataBase</span>
                    </a>
                  </li>
                </ul>
              </div>
            </li>

          </ul>
        </div>
      </div>
    </div>
    <!-- main-panel -->
    <div class="main-panel">
      <div class="content"></div>
      <footer class="footer">
        <div class="container-fluid">
          <nav class="pull-left">
            <ul class="nav">
              <li class="nav-item">
                <a class="nav-link" href="/AboutUs" >
                  Vinland
                </a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="#">
                  Help
