<!DOCTYPE html>
<html lang="en">
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>Admin Dashboard</title>
    <meta content='width=device-width, initial-scale=1.0, shrink-to-fit=no' name='viewport' />
    <link rel="icon" href="/Admin/Files/images/icon.ico" type="image/x-icon"/>
    <!-- Fonts and icons -->
    <script src="/Admin/Files/javascripts/Assets/plugin/webfont/webfont.min.js"></script>
    <script>
      WebFont.load({
        custom: {"families":["Flaticon", "Font Awesome 5 Solid", "Font Awesome 5 Regular", "Font Awesome 5 Brands", "simple-line-icons"],
          urls: ['/Admin/Files/stylesheets/Assets/fonts.min.css']},
        active: function() {
          sessionStorage.fonts = true;
        }
      });
    </script>

    <!-- CSS Files -->
    <link rel="stylesheet" href="/Admin/Files/stylesheets/Assets/bootstrap.min.css">
    <link rel="stylesheet" href="/Admin/Files/stylesheets/Assets/atlantis.min.css">
    <link rel="stylesheet" href="/Admin/Files/stylesheets/Admin/index.css">

  </head>
  <body data-background-color="dark">
  <div class="wrapper sidebar_minimize">
    <!-- Navbar -->
